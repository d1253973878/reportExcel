
.newtec-mycanvas>.canvas-head{
	position: absolute;
	top:0;
	height: 40px;
	width: 100%;
	background: #f3f3f7;
}
.newtec-mycanvas>.canvas-head>.top-title{
	font-size: 14px;
    padding: 10px 0;
    display: inline-block;
    background: #40434c;
    color: #fff;
    width: 61px;
    text-align: center;
}
.newtec-mycanvas>.canvas-head>.btn-div{
	padding: 5px;
}
.newtec-mycanvas>.canvas-head>.btn-div>.btn-blue{
	background: #0597e2;
	color: #fff;
}
.newtec-mycanvas>.canvas-head>.btn-div>.btn{
	height: 30px;
	padding: 3px 12px;
	margin-left: 10px;
}
.newtec-mycanvas>.canvas-head>.btn-div>.btn>img{
	width: 18px;
    margin-right: 8px;
    position: relative;
    top: -1px;
}
.newtec-mycanvas>.canvas-head>.btn-div>span{
	margin-right: 15px;
	padding: 5px;
	display: inline-block;
	cursor: pointer;
}
.newtec-mycanvas>.canvas-head>.btn-div>span.a-ative{
	background: #d5d5df;
	border-radius: 4px;
}
.newtec-mycanvas>.canvas-head>.btn-div>span>img{
	width: 20px;
}
.newtec-mycanvas>.canvas-main>.main-body.select-draging>.raphael-canvas{
	position: absolute;
	top:0;
	left:0;
	width: 100%;
	height:100%;
}
.newtec-mycanvas>.canvas-main>.main-body.select-draging>.raphael-canvas{
	z-index: 10;
}
.newtec-mycanvas>.canvas-left{
	position: absolute;
	top:40px;
	height: 100%;
	width: 60px;
	background: #f3f3f7;
}
.newtec-mycanvas>.canvas-left>.org-item{
	margin: 10px;
	text-align: center;
}
.bpm-body{
	position: absolute;
	top:0;
	left:0;
	bottom: 0;
	right: 0;
	min-height: 1200px;
}
.bpm-body>.h-line{
	position: absolute;
	top:0;
	height: 100%;
	background:#ddd;
	width: 1px;
}
.bpm-body>.v-line{
	position: absolute;
	left:0;
	width: 100%;
	background:#ddd;
	height: 1px;
}
.newtec-mycanvas>.menu-layout{
	position: absolute;
	left:10px;
	top:50px;
	bottom: 50px;
	width: 100px;
	background: #fff;
	z-index: 10;
	height: 400px;
	-webkit-box-shadow: 1px 1px 1px; -moz-box-shadow: 1px 1px 1px;box-shadow:1px 1px 1px; 
}
.newtec-mycanvas>.menu-layout>.m-title{
	text-align: center;
	font-weight: 600;
	background: #ddd;
    padding: 8px;
    border-bottom: 1px solid black;
}
.newtec-mycanvas.menu-layout>.m-body>.n-item{
	padding: 4px 9px;
    cursor: pointer;
    border: 3px dotted transparent;
     /*-moz-user-select:none;
	-webkit-user-select:none;
	-ms-user-select:none;
	-khtml-user-select:none;
	user-select:none;*/
	 margin: 0 2px;
	 border-radius: 6px;
}

.newtec-mycanvas>.menu-layout>.m-body>.n-item:hover{
	background: #dcdcdc;
}
.newtec-mycanvas>.menu-layout>.m-body>.n-item.active{
	border: 3px dotted #2277ee;
}
.newtec-mycanvas>.menu-layout>.m-body>.node-l{
	cursor: -webkit-grab;
}
.newtec-mycanvas>.menu-layout>.m-body>.n-line{
	border-bottom: 1px solid #515151;
	margin: 5px;
}
.newtec-mycanvas>.menu-layout>.m-body>.n-item>img{
	width: 25px;
	margin-right: 5px;
	position: relative;
    top: -2px;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item{
	cursor: pointer;
	text-align: center;
	position: absolute !important;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item.model1{
	border: 2px solid #8c7e7e;
    min-width: 80px;
    border-radius: 6px;
    height: 40px;
    cursor: pointer;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item.model2{
	border: 2px solid transparent;
    border-radius: 60px;
    height: 50px;
    width: 50px;
}

.newtec-mycanvas>.canvas-main>.main-body>.node-item.selected{
	border: 2px solid red;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item.model2.selected{
	border: 2px dotted red;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item>.node-menu,
.newtec-mycanvas>.canvas-main>.main-body>.node-item>.node-body{
	position: absolute;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item>.node-body{
	top:0;
	left:0;
	height: 100%;
	width: 100%;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item.model1>.model-play1{
	background: #fff;
    border-radius: 6px;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item.model2>.model-play2>img{
	margin-top: 3px;
    width: 80%;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item>.node-body{
	display: none;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item>.node-body>p{
	 position: absolute;
	 min-width: 100%;
	word-break:break-all;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item.n-clone{
	background: #e4e4e4;
    border: 2px dotted;
    opacity: 0.5;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item.n-clone>.node-body{
	display: none !important;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item>.model-play1>p{
	padding-top: 10px;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item.n-nowrap>.node-body>p{
	 white-space:nowrap;
}
.newtec-mycanvas>.canvas-main>.main-body>.node-item.model1>.model-play1,
.newtec-mycanvas>.canvas-main>.main-body>.node-item.model2>.model-play2{
	display: block;
}
