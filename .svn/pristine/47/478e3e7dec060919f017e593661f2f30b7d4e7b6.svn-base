.newtec-mycanvas{
	position: relative;
	width:100%;
	height:100%;
	position: relative;
	overflow: hidden;
}
.newtec-mycanvas>.canvas-main{
	position: absolute !important;
	left:0px;
	top: 0;
	right: 0px;
	bottom: 0px;
}
.newtec-mycanvas>.canvas-main.can-drag{
	
	left:-500px;
	top: 0;
	right: -500px;
	bottom: -500px;
	/*cursor: all-scroll;*/
}
.newtec-mycanvas>.canvas-main>.main-body>canvas{
	width:100%;
	height:100%;
	position: absolute;
	top: 0;
	left:0;
}
.newtec-mycanvas>.dropdown-menu>li{
	cursor: pointer;
}
.newtec-mycanvas>.dropdown-menu>li>i.icon{
	margin: 0 5px;
}

.newtec-mycanvas>.canvas-main>.main-body{
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left:0;
}
.newtec-mycanvas>.canvas-main>.main-body{
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left:0;
	 -moz-user-select:none;
	-webkit-user-select:none;
	-ms-user-select:none;
	-khtml-user-select:none;
	user-select:none;
}
.newtec-mycanvas>.canvas-main>.main-body .mobile-item{
	position: absolute;
}
.newtec-mycanvas>.canvas-main>.main-body .mobile-item.mb-item{
	width:10px;
	height: 10px;
	background: red;
	border-radius: 50px;
}
.newtec-mycanvas>.canvas-main>.main-body>#raphael-canvas{
	width:100%;
	height:100%;
	position: absolute;
	top: 0;
	left:0;
}
